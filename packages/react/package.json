{
  "name": "@tianniu-monitor/react",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build:watch": "pnpm build:watch:transpile & pnpm build:watch:types",
    "build:watch:transpile": "tsup --watch",
    "build:watch:types": "tsc -p tsconfig.json --emitDeclarationOnly --watch",
    "build": "pnpm build:transpile && pnpm build:types",
    "build:transpile": "tsup",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rimraf build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Tianniu",
  "license": "ISC",
  "description": "",
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "dependencies": {
    "@tianniu-monitor/browser-utils": "workspace:*",
    "@tianniu-monitor/monitor-core": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "@types/react": "^18.0.0",
    "tsup": "8.5.1"
  }
}
